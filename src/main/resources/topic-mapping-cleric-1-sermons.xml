<?xml version="1.1" encoding="UTF-8"?>
<topic-strategies xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="./topic-mapping.xsd">
    <strategy name="Remove last lines from Page 74" index="1">
        <strategyClass>StrategyRemoveLinesFromPage</strategyClass>
        <params>
            <parameter>
                <key>applyToPage</key>
                <value>74</value>
            </parameter>
            <parameter>
                <key>fromLine</key>
                <value>77</value>
            </parameter>
            <parameter>
                <key>untilLine</key>
                <value>79</value>
            </parameter>
        </params>
    </strategy>

    <strategy name="Split check of first Sermon from headline on page 74" index="1">
        <strategyClass>StrategySplitLineAfterPosition</strategyClass>
        <params>
            <parameter>
                <key>applyToPage</key>
                <value>74</value>
            </parameter>
            <parameter>
                <key>splitLine</key>
                <value>27</value>
            </parameter>
            <parameter>
                <key>splitAfterPosition</key>
                <value>26</value>
            </parameter>
        </params>
    </strategy>

    <strategy name="Remove Predigt headline from Page 74" index="1">
        <strategyClass>StrategyRemoveSingleLineFromPage</strategyClass>
        <params>
            <parameter>
                <key>applyToPage</key>
                <value>74</value>
            </parameter>
            <parameter>
                <key>lineNo</key>
                <value>27</value>
            </parameter>
        </params>
    </strategy>
</topic-strategies>