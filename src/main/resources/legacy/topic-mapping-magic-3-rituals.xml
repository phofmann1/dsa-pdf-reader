<?xml version="1.1" encoding="UTF-8"?>
<topic-strategies xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:noNamespaceSchemaLocation="./topic-mapping.xsd">
    <strategy name="Switch content on page 141" index="2">
        <strategyClass>
            StrategyPutToEndOfPage
        </strategyClass>
        <params>
            <parameter>
                <key>applyToPage</key>
                <value>141</value>
            </parameter>
            <parameter>
                <key>startEntryLineNo</key>
                <value>3</value>
            </parameter>
            <parameter>
                <key>endEntryLineNo</key>
                <value>67</value>
            </parameter>
        </params>
    </strategy>
    <strategy name="Remove Box from Page 144" index="1">
        <strategyClass>StrategyRemoveLinesFromPage</strategyClass>
        <params>
            <parameter>
                <key>applyToPage</key>
                <value>144</value>
            </parameter>
            <parameter>
                <key>fromLine</key>
                <value>88</value>
            </parameter>
        </params>
    </strategy>
</topic-strategies>